<div id="listr">
    {% for d in data %}
        <div class="col-md-6">
            <a href="detail_page.html" class="strip_list">
                <div class="ribbon_1">Popular</div>
                <div class="desc">
                    <div class="thumb_strip">
                        <img src="{{ asset('frontend/img/thumb_restaurant.jpg') }}" alt="">
                    </div>
                    <div class="rating">
                        <i class="icon_star voted"></i><i class="icon_star voted"></i><i class="icon_star voted"></i><i class="icon_star voted"></i><i class="icon_star"></i>
                    </div>
                    <h3>{{ d.nomResto }}</h3>
                    <div class="type">
                        {{ d.typeResto }}
                    </div>
                    <div class="location">
                        {{ d.adresseResto }} <span class="closing">ferm√© maintenat</span>
                    </div>
                    <ul>
                        <li><i class="icon_clock_alt ok"></i>{{ d.ouverture }}</li>
                        <li><i class="icon_clock_alt no"></i>{{ d.fermeture }}</li>
                    </ul>
                </div><!-- End desc-->
            </a><!-- End strip_list-->

        </div>
    {% endfor %}
</div>
